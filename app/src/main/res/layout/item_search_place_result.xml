<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toStartOf="@+id/imageView">
    <TextView
            android:text="서울 소극장"
            android:textSize="16dp"
            android:textColor="#000"
            android:fontFamily="@font/seoulnamsan_eb"
            android:layout_width="wrap_content"
            android:layout_height="16dp" android:id="@+id/searchResult_place_name_tv"
            android:layout_weight="1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="서울특별시 종로구 동숭동"
            android:fontFamily="@font/seoulnamsan_eb"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp" android:id="@+id/searchResult_place_place_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/searchResult_place_name_tv" android:layout_marginTop="4dp"/>
    <TextView
            android:text="02-222-3333"
            android:layout_weight="1"
            android:fontFamily="@font/seoulnamsan_eb"
            android:layout_width="match_parent"
            android:layout_height="0dp" android:id="@+id/searchResult_show_number_tv"
            android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/searchResult_place_place_tv"
            app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>