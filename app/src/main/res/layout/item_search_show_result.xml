<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
android:layout_width="match_parent"
android:layout_height="wrap_content" app:layout_constraintStart_toStartOf="parent"

app:layout_constraintTop_toTopOf="parent"
app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toStartOf="@+id/imageView">
<TextView
        android:text="위대한 쇼맨"
        android:textSize="16dp"
        android:textColor="#000"
        android:fontFamily="@font/seoulnamsan_eb"
        android:layout_width="wrap_content"
        android:layout_height="16dp" android:id="@+id/searchResult_place_name_tv"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
<TextView
        android:text="서울 소극장"
        android:fontFamily="@font/seoulnamsan_eb"
        android:layout_weight="1"
        android:textSize="16dp"
        android:textColor="#000"
        android:layout_width="match_parent"
        android:layout_height="0dp" android:id="@+id/searchResult_place_place_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchResult_place_name_tv" android:layout_marginTop="4dp"/>
<TextView
        android:text="뮤지컬"
        android:fontFamily="@font/seoulnamsan_eb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/searchResult_place_genre_tv" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="4dp" app:layout_constraintTop_toBottomOf="@+id/searchResult_place_place_tv"/>
<TextView
        android:text=" / "
        android:fontFamily="@font/seoulnamsan_eb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6" app:layout_constraintStart_toEndOf="@+id/searchResult_place_genre_tv"
        android:layout_marginTop="4dp" app:layout_constraintTop_toBottomOf="@+id/searchResult_place_place_tv"/>
<TextView
        android:text="만 12세 이상"
        android:fontFamily="@font/seoulnamsan_eb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/searchDetail_place_age_tv" android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/searchResult_place_place_tv"
        app:layout_constraintStart_toEndOf="@+id/textView6"/>
</androidx.constraintlayout.widget.ConstraintLayout>
